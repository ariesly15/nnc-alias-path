{
    "name": "nnc-alias-path",
    "displayName": "nnc-alias-path",
    "description": "nnc-alias-path",
    "version": "1.0.1",
    "engines": {
        "vscode": "^1.34.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./extension.js",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "nnc-alias-path",
            "properties": {
                "nncAliasPath.isNNC": {
                    "type": "boolean",
                    "default": true,
                    "description": "是否为nanachi,默认为true时不读取其他alias配置"
                },
                "nncAliasPath.configPath": {
                    "type": "string",
                    "default": "",
                    "description": "配置包含有alias的json文件"
                },
                "nncAliasPath.key": {
                    "type": "string",
                    "default": "pathAlias",
                    "description": "配置的别名字段默认读取 .pathAlias"
                }
            }
        },
        "commands": [
            {
                "command": "extension.helloWorld",
                "title": "Hello World"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.3.1",
        "vscode": "^1.1.28",
        "eslint": "^5.13.0",
        "@types/node": "^10.12.21",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "glob": "^7.1.4"
    }
}
